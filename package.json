{
  "private": true,
  "name": "keynote-code-hightlight",
  "version": "1.0.0",
  "description": "",
  "main": "src/electron/index.js",
  "homepage": "./",
  "scripts": {
    "start": "NODE_ENV=development react-scripts start & npm run electron",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "electron": "electron .",
    "lint": "eslint .",
    "icon:export": "sketchtool export slices design/icon.sketch --output=./resources/Icon.iconset",
    "icon:create": "iconutil -c icns ./resources/Icon.iconset",
    "menuicon:export": "sketchtool export artboards design/iconTemplate.sketch --output=./resources",
    "icon": "npm run icon:export && npm run icon:create && npm run menuicon:export",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [],
  "author": "Andrey Okonetchnikov <andrey@okonet.ru>",
  "license": "ISC",
  "dependencies": {
    "electron": "^1.4.15",
    "electron-positioner": "^3.0.0",
    "electron-settings": "^2.2.2",
    "emphasize": "^1.2.0",
    "execa": "^0.6.0",
    "highlight-rtf-renderer": "file:./lib",
    "highlight.js": "^9.10.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-lowlight": "^1.0.4",
    "styled-components": "^1.4.4",
    "system-font-families": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-okonet": "^3.0.2",
    "husky": "^0.13.1",
    "lint-staged": "^3.3.1",
    "prettier": "^0.22.0",
    "react-scripts": "0.9.5"
  },
  "eslintConfig": {
    "extends": "okonet",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "no-console": "off",
      "strict": [
        "error",
        "safe"
      ]
    }
  }
}
