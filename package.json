{
  "private": true,
  "name": "codestage",
  "version": "0.0.1",
  "description": "Hassle free code highlighting macOS helper app for your Keynote slides",
  "main": "src/electron/index.js",
  "homepage": "./",
  "build": {
    "appId": "com.koolesache.CodeStage",
    "asarUnpack": [
      "**/*.applescript"
    ],
    "directories": {
      "buildResources": "resources"
    }
  },
  "scripts": {
    "start": "concurrently --kill-others \"neutrino start\" \"npm run electron\"",
    "build": "neutrino build",
    "pack": "npm run icon && npm run build && build --dir",
    "dist": "npm run icon && npm run build && build",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "electron": "electron .",
    "lint": "eslint src/",
    "icon:export": "sketchtool export slices design/icon.sketch --output=./resources/icon.iconset",
    "icon:create": "iconutil -c icns ./resources/icon.iconset",
    "menuicon:export": "sketchtool export artboards design/iconTemplate.sketch --output=./public",
    "icon": "npm run icon:export && npm run icon:create && npm run menuicon:export",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "git-exec-and-restage eslint --fix --",
      "jest --findRelatedTests --bail"
    ]
  },
  "keywords": [],
  "author": "Andrey Okonetchnikov <andrey@okonet.ru>",
  "license": "MPL-2.0",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "brace": "^0.10.0",
    "electron-clipboard-watcher": "^1.0.1",
    "electron-debug": "^1.1.0",
    "electron-is-dev": "^0.1.2",
    "electron-log": "^2.2.3",
    "electron-positioner": "^3.0.0",
    "electron-redux": "^1.2.10",
    "electron-settings": "^3.0.14",
    "emphasize": "^1.2.0",
    "execa": "^0.6.0",
    "hazardous": "^0.2.3",
    "highlight-rtf-renderer": "file:./lib",
    "key-event-to-string": "^1.1.1",
    "react": "^15.4.2",
    "react-desktop": "^0.3.0",
    "react-dom": "^15.4.2",
    "react-focus-onkeydown": "^1.2.0",
    "react-hot-loader": "next",
    "react-lowlight": "^1.0.4",
    "react-prism": "^4.1.0",
    "react-redux": "^5.0.5",
    "react-virtualized": "^9.9.0",
    "react-with-timer-hoc": "^0.2.3",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "rehype-parse": "^3.1.0",
    "rehype-react": "^3.0.0",
    "rehype-stringify": "^3.0.0",
    "strip-indent": "^2.0.0",
    "styled-components": "^1.4.4",
    "system-font-families": "^1.0.0",
    "unified": "^6.1.3",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "concurrently": "^3.5.0",
    "devtron": "^1.4.0",
    "electron": "1.8.1",
    "electron-builder": "19.31.1",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^3.19.0",
    "eslint-config-okonet": "^4.0.2",
    "git-exec-and-restage": "^1.0.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.0.3",
    "neutrino": "^7.0.1",
    "neutrino-preset-react": "^7.0.1",
    "prettier": "^1.5.3"
  },
  "eslintConfig": {
    "extends": "okonet",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "no-console": "off",
      "strict": [
        "error",
        "safe"
      ]
    }
  }
}
