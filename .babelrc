{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "electron": "1.8"
            },
            "useBuiltIns": true,
            "debug": true
          }
        ],
        "react"
      ],
      "plugins": [
        "react-hot-loader/babel",
        "transform-object-rest-spread",
        "transform-class-properties",
        "styled-components"
      ],
      "sourceMaps": "inline"
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "electron": "1.8"
            },
            "useBuiltIns": true
          }
        ],
        "react"
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties",
        "styled-components"
      ],
      "sourceMaps": "none"
    }
  }
}
